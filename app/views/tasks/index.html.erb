<% provide(:title, "Users") %>
<h1>利用者一覧</h1>
<div class="row">
  <div class="col-md-10 col-md-offset-1">
    
    <table class="table table-condensed table-hover" id="table-users">
        <thead>
            <th>名前</th>
        </thead>
        <tbody>
          <% @tasks.each do |task| %>
             <td><%= task.name %></td>
             <td></td>
             <td></td>
          <% end %>
        </tbody>
    </table>
   
  </div>
</div>